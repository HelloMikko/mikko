{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Weight",
          "name": "weight",
          "type": "number",
          "default": "1"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "default": "lorem lipson"
        },
        {
          "title": "Summery",
          "name": "summery",
          "type": "string",
          "default": "lorem lipson"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "default": "Mikko"
        },
        {
          "title": "Author Link",
          "name": "authorLink",
          "type": "string",
          "default": "https://mikko.codes"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Resources",
          "name": "resources",
          "type": "list"
        },
        {
          "title": "Lightgallery",
          "name": "lightgallery",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Hero Image",
          "name": "featuredimage",
          "type": "image"
        },
        {
          "title": "Images",
          "name": "images",
          "type": "list"
        },
        {
          "title": "Divider",
          "name": "divider",
          "type": "boolean",
          "default": "---"
        },
        {
          "title": "Section title",
          "name": "sectionTitleWithDescription",
          "description": "This is just a dummy description to test out the field description",
          "type": "heading"
        },
        {
          "title": "License",
          "name": "license",
          "type": "taxonomy",
          "taxonomyId": "license"
        },
        {
          "title": "Last modified",
          "name": "lastmod",
          "type": "datetime",
          "default": "{{now}}"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "list",
          "choices": [
            "HTML",
            "CSS",
            "JavaScript",
            "React",
            "Vue",
            "Angular",
            "jQuery",
            "Bootstrap",
            "Sass",
            "LESS",
            "Responsive Design",
            "Web Accessibility",
            "Progressive Web Apps",
            "Performance Optimization",
            "UX Design",
            "UI Design",
            "Version Control",
            "GitHub",
            "GitLab",
            "Front-end Frameworks",
            "API Integration",
            "Webpack",
            "Gulp",
            "Grunt",
            "npm",
            "Yarn",
            "Testing",
            "Browser Compatibility",
            "SEO",
            "Web Components",
            "CSS Grid",
            "Tailwind CSS",
            "Materialwind",
            "Bootstrap"
          ]
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.taxonomy.tags": [
    "API Integration",
    "Angular",
    "Bootstrap",
    "Browser Compatibility",
    "CSS",
    "CSS Grid",
    "Front-end Frameworks",
    "GitHub",
    "GitLab",
    "Grunt",
    "Gulp",
    "HTML",
    "JavaScript",
    "LESS",
    "Material + Tailwind",
    "Performance Optimization",
    "Progressive Web Apps",
    "React",
    "Responsive Design",
    "SEO",
    "Sass",
    "Tailwind CSS",
    "Testing",
    "UI Design",
    "UX Design",
    "Version Control",
    "Vue",
    "Web Accessibility",
    "Web Components",
    "Webpack",
    "Yarn",
    "jQuery",
    "npm"
  ],
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "license",
      "options": [
        "Public Domain (CC0)",
        "Attribution (CC BY)",
        "Attribution-ShareAlike (CC BY-SA)",
        "Attribution-NoDerivs (CC BY-ND)",
        "Attribution-NonCommercial (CC BY-NC)",
        "Attribution-NonCommercial-ShareAlike (CC BY-NC-SA)",
        "MIT License",
        "Apache License 2.0",
        "GNU General Public License (GPL)",
        "Custom License"
      ]
    }
  ],
  "frontMatter.taxonomy.categories": [
    "Articles",
    "Documentation",
    "Project",
    "Snippets"
  ],
  "frontMatter.content.snippets": {
    "Version Change": {
      "description": "This example will add a version tag with the version numer in it",
      "body": "{{< version 0.2.0 changed >}}",
      "fields": []
    },
    "Codebox Snippet": {
      "description": "This example will add a snippet inside a codebox with highlights",
      "body": [
        "```markdown",
        "{{</* style \"text-align:right; strong{color:#00b1ff;}\" */>}}",
        "This is a **right-aligned** paragraph.",
        "{{</* /style */>}}",
        "```"
      ],
      "fields": []
    },
    "Image": {
      "description": "This example will add an image snippet in markdown (media snippet is enabled)",
      "body": [
        "```markdown",
        "{{</* image src=\"/images/lighthouse.jpg\" caption=\"Lighthouse (`image`)\" src_s=\"/images/lighthouse-small.jpg\" src_l=\"/images/lighthouse-large.jpg\" */>}}",
        "```"
      ],
      "isMediaSnippet": true
    },
    "Default Banner": {
      "description": "This example will add a normal banner",
      "body": [
        "{{< admonition >}}",
        "Hugo **extended** version is necessary for `style` shortcode.",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Abstract Banner": {
      "description": "This will add an Abstract Banner",
      "body": [
        "{{< admonition abstract >}}",
        "An **abstract** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Info Banner": {
      "description": "This will add a Info Banner",
      "body": [
        "{{< admonition info >}}",
        "A **info** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Tip Banner": {
      "description": "This will add a Tip Banner",
      "body": [
        "{{< admonition tip >}}",
        "A **tip** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Success Banner": {
      "description": "This will add a Success Banner",
      "body": [
        "{{< admonition success >}}",
        "A **success** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Question Banner": {
      "description": "This will add a question banner",
      "body": [
        "{{< admonition question >}}",
        "A **question** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Warning Banner": {
      "description": "This will add a warning banner",
      "body": [
        "{{< admonition warning >}}",
        "A **warning** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Failure Banner": {
      "description": "This will add a failure banner",
      "body": [
        "{{< admonition failure >}}",
        "A **failure** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Danger Banner": {
      "description": "This will add a danger banner",
      "body": [
        "{{< admonition danger >}}",
        "A **danger** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Bug Banner": {
      "description": "This will add a bug banner",
      "body": [
        "{{< admonition bug >}}",
        "A **bug** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Example Banner": {
      "description": "This will add an example banner",
      "body": [
        "{{< admonition example >}}",
        "An **example** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Quote Banner": {
      "description": "This will add a quote banner",
      "body": [
        "{{< admonition quote >}}",
        "A **quote** banner",
        "{{< /admonition >}}"
      ],
      "fields": []
    },
    "Person w/Avatar": {
      "description": "This example will add a person and their avatar snippet",
      "body": "{{< person url=\"https://evgenykuznetsov.org\" name=\"Evgeny Kuznetsov\" nick=\"nekr0z\" text=\"author of this shortcode\" picture=\"https://evgenykuznetsov.org/img/avatar.jpg\" >}}",
      "isMediaSnippet": true
    },
    "Person": {
      "description": "This example will add a person without their avatar",
      "body": "{{< person \"https://mikko.codes/\" Dillon \"author of the MikkoCodes theme\" >}}",
      "fields": []
    },
    "HTML Snippet": {
      "description": "This will add a codebox with HTML in it",
      "body": [
        "```html",
        "<figure>",
        "    <img src=\"/images/lighthouse.jpg\"/>",
        "    <figcaption>",
        "        <h4>Lighthouse (figure)</h4>",
        "    </figcaption>",
        "</figure>",
        "```"
      ],
      "fields": []
    },
    "Gist Box": {
      "description": "This example will add a gist snippet using the url extention",
      "body": "{{< gist codemikko 12f609b70ff7edd47f329c2f683c801d >}}",
      "fields": []
    },
    "Tweet Post": {
      "description": "This will add an existing twitter post with the tweets url extention",
      "body": "{{< tweet 917359331535966209 >}}",
      "fields": []
    },
    "Vimeo": {
      "description": "This snippet will add a vimeo video using its video extention",
      "body": "{{< vimeo 146022717 >}}",
      "fields": [],
      "isMediaSnippet": true
    },
    "Youtube Video": {
      "description": "This example will add a youtube video using the video id",
      "body": [
        "{{< youtube w7Ft2ymGmfc >}}",
        ""
      ],
      "fields": []
    },
    "Banner with Title": {
      "description": "This snippet shows as an example using a banner with a title and a codebox",
      "body": [
        "{{< admonition tip \"Tips about CDN Configuration\" >}}",
        "{{< version 0.2.7 changed >}}",
        "",
        "```toml",
        "[params.cdn]",
        "  # CDN data file name, disabled by default",
        "  # [\"jsdelivr.yml\"]",
        "  data = \"\"",
        "````"
      ],
      "fields": []
    },
    "Add Badges With Style": {
      "description": "This example will add an inline style for badges",
      "body": [
        "{{< style \"img { height: 1.25rem; }\" >}}",
        "[![GitHub release (latest by date)](https://img.shields.io/github/v/release/codemikko/mikkocodes/?style=flat-square)](https://github.com/codemikko/mikkocodes//releases)",
        "[![Hugo](https://img.shields.io/badge/Hugo-%5E0.62.0-ff4088?style=flat-square&logo=hugo)](https://gohugo.io/)",
        "[![License](https://img.shields.io/github/license/codemikko/mikkocodes/?style=flat-square)](https://github.com/codemikko/mikkocodes//blob/master/LICENSE)",
        "[![GitHub stars](https://img.shields.io/github/stars/codemikko/mikkocodes/?style=social)](https://github.com/codemikko/mikkocodes/)",
        "[![GitHub forks](https://img.shields.io/github/forks/codemikko/mikkocodes/?style=social)](https://github.com/codemikko/mikkocodes//fork)",
        "{{< /style >}}"
      ],
      "isMediaSnippet": true
    },
    "Adding Inline Style": {
      "description": "This will add an inline style to your content",
      "body": [
        "{{< style >}}",
        "BLAH BLAH BLAH",
        "{{< /style >}}"
      ],
      "fields": []
    },
    "Adding a Banner with Emoji's": {
      "description": "This example will add a banner with emoji's at the start with content in it",
      "body": [
        "{{< style \"img { height: 1.25rem; }\" >}}",
        "[![GitHub release (latest by date)](https://img.shields.io/github/v/release/codemikko/mikkocodes/?style=flat-square)](https://github.com/codemikko/mikkocodes//releases)",
        "[![Hugo](https://img.shields.io/badge/Hugo-%5E0.62.0-ff4088?style=flat-square&logo=hugo)](https://gohugo.io/)",
        "[![License](https://img.shields.io/github/license/codemikko/mikkocodes/?style=flat-square)](https://github.com/codemikko/mikkocodes//blob/master/LICENSE)",
        "[![GitHub stars](https://img.shields.io/github/stars/codemikko/mikkocodes/?style=social)](https://github.com/codemikko/mikkocodes/)",
        "[![GitHub forks](https://img.shields.io/github/forks/codemikko/mikkocodes/?style=social)](https://github.com/codemikko/mikkocodes//fork)",
        "{{< /style >}}",
        "",
        "> [:(far fa-kiss-wink-heart fa-fw): MikkoCodes](https://github.com/codemikko/mikkocodes/) is a **clean**, **elegant** but **advanced** blog theme for [Hugo](https://gohugo.io/) developed by {{< person \"https://mikko.codes/\" Dillon \"author of the MikkoCodes theme\" \"/Dillon.png\" >}}.",
        ">",
        "> It is based on the original [LeaveIt Theme](https://github.com/liuzc/LeaveIt) and [KeepIt Theme](https://github.com/Fastbyte01/KeepIt)."
      ],
      "isMediaSnippet": true
    },
    "Raw Codes": {
      "description": "When adding a raw code to display it such as showing a demo use:",
      "body": [
        "{{< raw >}}",
        "Add HTML Embeds here",
        "{{< /raw >}}"
      ],
      "isMediaSnippet": true,
      "fields": []
    },
    "Mermaid": {
      "description": "Mermaid to create graphs(see example below) and check DOCS for more demos https://mermaid.js.org/syntax/examples.html",
      "body": [
        "{{< mermaid \"codemikko/mikkocodes\" >}}",
        "    gitGraph",
        "       commit",
        "       commit id: \"Normal\" tag: \"v1.0.0\"",
        "       commit",
        "       commit id: \"Reverse\" type: REVERSE tag: \"RC_1\"",
        "       commit",
        "       commit id: \"Highlight\" type: HIGHLIGHT tag: \"8.8.4\"",
        "       commit",
        "{{< /mermaid >}}"
      ],
      "fields": []
    }
  },
  "frontMatter.content.pageFolders": [
    {
      "title": "default",
      "path": "[[workspace]]/content",
      "originalPath": "[[workspace]]/content"
    },
    {
      "title": "about",
      "path": "[[workspace]]/content/about",
      "originalPath": "[[workspace]]/content/about"
    },
    {
      "title": "documentation",
      "path": "[[workspace]]/content/categories/documentation",
      "originalPath": "[[workspace]]/content/categories/documentation"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts",
      "originalPath": "[[workspace]]/content/posts"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/content/projects",
      "originalPath": "[[workspace]]/content/projects"
    },
    {
      "title": "snippets",
      "path": "[[workspace]]/content/snippets",
      "originalPath": "[[workspace]]/content/snippets"
    }
  ],
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.experimental": true
}